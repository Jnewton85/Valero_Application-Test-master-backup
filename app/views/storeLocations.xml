<Alloy>
	<Window class="container" title="Store Locations">
		<Menu platform="android">
			<MenuItem id="label" title="Home" onClick="index"></MenuItem>
			<MenuItem id ="label" title="Nearby Locations" onClick="nearbyLocations"></MenuItem>
			<MenuItem id="label" title="Trip Planner" onClick="tripPlanner"></MenuItem>
			</Menu>
		<Picker id="stateList">
			<PickerColumn title="State">
				<PickerRow title="Texas"></PickerRow>
				<PickerRow title="New Mexico"></PickerRow>
				<PickerRow title="Florida"></PickerRow>
				<PickerRow title="California"></PickerRow>
			</PickerColumn>
		</Picker>
		<Picker id="cityList">
			<PickerColumn title="City">
				<PickerRow title="Amarillo"></PickerRow>
				<PickerRow title="Houston"></PickerRow>
				<PickerRow title="Dallas"></PickerRow>
				<PickerRow title="Austin"></PickerRow>
			</PickerColumn>
		</Picker>
	</Window>
</Alloy>